<?xml version="1.0" encoding="UTF-8"?>
    <!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>
	<package name="action" namespace="/" extends="util">
			<action name="articleIndex" class="articleAction" method="articleIndex">
				<!--
				<interceptor-ref name="authority"></interceptor-ref>
				<interceptor-ref name="defaultStack"></interceptor-ref>
				-->
				<result name="article_index">/views/index/index.jsp</result>
			</action>
			
			<action name="articleCategory" class="articleAction" method="articleCategory">
				<result name="article_category">/views/index/category.jsp</result>
			</action>
			
			<action name="articleDetail" class="articleAction" method="articleDetail">
				<result name="article_detail">/views/index/detail.jsp</result>
			</action>
	</package>
</struts>


